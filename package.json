    "setil": "^0.0.1-a",
    "zod-opts": "^0.1.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.1",
    "@types/node": "^24.9.1",
    "esbuild-register": "^3.6.0",
    "typescript": "^5.3.3"
